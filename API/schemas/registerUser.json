{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "register user",
  "description": "This schema is for registering a new user",
  "default": {},
  "examples": [
      {
          "username": "34567",
          "password": "dslkafjlj"
      }
  ],
  "required": [
      "username",
      "password"
  ],
  "properties": {
      "username": {
          "$id": "#/properties/username",
          "type": "string",
          "title": "The username schema",
          "description": "username for the new user",
          "default": "",
          "minLength": 2,
          "maxLength": 20,
          "examples": [
              "34567"
          ]
      },
      "password": {
          "$id": "#/properties/password",
          "type": "string",
          "title": "The password schema",
          "description": "password for the new user",
          "default": "",
          "minLength": 5,
          "maxLength": 50,
          "examples": [
              "dslkafjlj"
          ]
      }
  },
  "additionalProperties": false
}